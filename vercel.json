{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "git": {
    "deploymentEnabled": true
  },
  "crons": [
    {
      "path": "/api/workflows/process",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/reviewflow/sync-all",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/pipelines/daily",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/tours/reminders",
      "schedule": "*/15 * * * *"
    }
  ]
}




